<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto"
                                             xmlns:tools="http://schemas.android.com/tools"
                                             android:layout_width="match_parent"
                                             android:layout_height="match_parent"
                                             tools:context=".MainActivity">
    <TextView
            android:id="@+id/do1"
            android:text="도"
            android:background="@android:color/holo_red_dark"
            android:layout_width="50dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="16dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="16dp"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            android:textColor="@android:color/white"
            android:gravity="center_horizontal|center_vertical" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/re"/>

    <TextView
            android:id="@+id/re"
            android:text="레"
            android:background="@android:color/holo_orange_dark"
            android:layout_width="50dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="24dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="24dp"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            android:textColor="@android:color/white"
            android:gravity="center_horizontal|center_vertical" app:layout_constraintVertical_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/do1" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/mi"/>

    <TextView
            android:id="@+id/mi"
            android:text="미"
            android:background="@android:color/holo_orange_light"
            android:layout_width="50dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="32dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="32dp"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            android:textColor="@android:color/white"
            android:gravity="center_horizontal|center_vertical" app:layout_constraintVertical_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/re" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/fa"/>

    <TextView
            android:id="@+id/fa"
            android:text="파"
            android:background="@android:color/holo_green_light"
            android:layout_width="50dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="40dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="40dp"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            android:textColor="@android:color/white"
            android:gravity="center_horizontal|center_vertical" app:layout_constraintVertical_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/mi" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/sol"/>

    <TextView
            android:id="@+id/sol"
            android:text="솔"
            android:background="@android:color/holo_blue_light"
            android:layout_width="50dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="48dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="48dp"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            android:textColor="@android:color/white"
            android:gravity="center_horizontal|center_vertical" app:layout_constraintVertical_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/fa" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/la"/>

    <TextView
            android:id="@+id/la"
            android:text="라"
            android:background="@android:color/holo_blue_dark"
            android:layout_width="50dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="56dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="56dp"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            android:textColor="@android:color/white"
            android:gravity="center_horizontal|center_vertical" app:layout_constraintVertical_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/sol" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/si"/>

    <TextView
            android:id="@+id/si"
            android:text="시"
            android:background="@android:color/holo_purple"
            android:layout_width="50dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="64dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="64dp"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            android:textColor="@android:color/white"
            android:gravity="center_horizontal|center_vertical" app:layout_constraintVertical_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/la" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/do2"/>

    <TextView
            android:id="@+id/do2"
            android:text="도"
            android:background="@android:color/holo_red_dark"
            android:layout_width="50dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="72dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="72dp"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            android:textColor="@android:color/white"
            android:gravity="center_horizontal|center_vertical" app:layout_constraintVertical_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/si" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toEndOf="parent"/>

</android.support.constraint.ConstraintLayout>